@model TechInvent.DM.Models.CabinetEquipment

@{
    ViewData["Title"] = "Удалить";
}

<h3>Уверены что хотите удалить?</h3>
<div>
    <h4>Оборудование</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Название
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Name)
        </dd>
        <dt class = "col-sm-2">
            Инвентарный номер
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.InventNumber)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="IdInventStuff" />
        <input type="submit" value="Удалить" class="btn btn-danger" />
        <a class="btn btn-primary" asp-action="Index">Назад к списку</a>
    </form>
</div>
